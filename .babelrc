{
  "presets": [
     "@babel/preset-env",
     "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "transform-class-properties",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["import", { "libraryName": "antd","libraryDirectory": "es","style": "true" }]
  ]
}
